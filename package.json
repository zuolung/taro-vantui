{
  "name": "taro-vantui",
  "version": "0.0.3",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "author": "zuolung <zuolung@126.com>",
  "description": "一套适用于Taro3的vantui组件库",
  "homepage": "https://zuolung.github.io/taro-vantui",
  "license": "MIT",
  "scripts": {
    "iconfont": "npx antm-icon --font-family antmuifont --input-path https://at.alicdn.com/t/font_2744513_7eeet2akdo.css --output-path src/style/components/iconfont.less",
    "_clean": "npx rimraf dist",
    "watch": "npx rollup -c ./rollup.watch.config.js -w",
    "docs:dev": "cross-env NODE_ENV=development webpack --config ./site/docs/config/dev-run.js",
    "docs:build": "cross-env NODE_ENV=production webpack --config ./site/docs/config/webpack.pro.config.js",
    "build": "npx rollup -c ./rollup.config.js",
    "deploy": "gh-pages -d dist-site"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "taroui",
    "taro",
    "component"
  ],
  "repository": {
    "type": "https",
    "url": "https://github.com/zuolung/taro-vantui"
  },
  "bugs": {
    "url": "https://github.com/zuolung/taro-vantui/issues"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6.4",
    "yarn": ">=1.22"
  },
  "browserslist": [
    "Chrome >= 35",
    "ChromeAndroid >= 35",
    "iOS >= 8",
    "Safari >= 8",
    "Android >= 4.1",
    "QQAndroid >= 4.1",
    "UCAndroid >= 4.1"
  ],
  "peerDependencies": {
    "@tarojs/components": ">=3.3.0",
    "@tarojs/taro": ">=3.3.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.7",
    "@vant/icons": "^1.7.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.6",
    "highlight.js": "^11.2.0",
    "lodash.flow": "^3.5.0",
    "markdown-it": "^12.2.0",
    "prop-types": "^15.7.2",
    "rc-util": "^5.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "cross-env": "^7.0.3",
    "@antmjs/babel-preset": "^1.14.0",
    "@antmjs/iconfont": "1.9.4",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tarojs/components": "^3.3.0",
    "@tarojs/taro": "^3.3.0",
    "@types/lodash.flow": "^3.5.6",
    "@types/react": "^17.0.17",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "copy-dir": "^1.3.0",
    "css-loader": "^6.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "gulp": "^4.0.2",
    "handlebars": "^4.7.7",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "less": "^4.1.2",
    "less-loader": "^10.0.1",
    "markdown-to-ast": "^6.0.3",
    "mini-css-extract-plugin": "^2.4.2",
    "postcss": "^7.0.0",
    "postcss-loader": "^6.1.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "style",
          "chore",
          "typings",
          "docs",
          "refactor",
          "test"
        ]
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "新功能（feature）"
        },
        "fix": {
          "description": "修补bug"
        },
        "style": {
          "description": "格式（不影响代码运行的变动）"
        },
        "chore": {
          "description": "构建过程或辅助工具的变动"
        },
        "typings": {
          "description": "Typescript 类型错误"
        },
        "docs": {
          "description": "文档（documentation）"
        },
        "refactor": {
          "description": "重构（既不是新增功能，也不是修改bug的代码变动）"
        },
        "test": {
          "description": "增加或修改测试用例"
        }
      }
    }
  }
}
